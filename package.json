{
  "name": "@cfl/simple-platform-server",
  "version": "0.1.0",
  "cfl": {
    "group": {
      "name": "labs"
    }
  },
  "description": "Simple server for web apps using the CoreFiling True North Platform APIs.",
  "main": "build/serve.js",
